{
  "name": "js-stack-from-scratch",
  "version": "0.0.1",
  "description": "Just learning the React Stack",
  "main": "index.js",
  "module": "main.js",
  "author": "Ezra Schwepker",
  "license": "MIT",
  "private": false,
  "scripts": {
